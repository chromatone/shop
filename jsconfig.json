{
  "include": [
    "./**/*.*"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "jsx": "preserve",
    "esModuleInterop": true,
    "target": "esnext",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "allowJs": true,
    "isolatedModules": true,
    "skipLibCheck": true,
    "strict": true,
    "allowSyntheticDefaultImports": true,
  },
  "vueCompilerOptions": {
    "vitePressExtensions": [
      ".md"
    ],
    "plugins": [
      "@vue/language-plugin-pug"
    ]
  }
}